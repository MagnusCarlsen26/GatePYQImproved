%{
#include "y.tab.h"
int step;
%}
%%
"print"				   {return print;}
"exit"				   {return exit_command;}
[a-zA-Z]			   {yylval.id = yytext[0]; return identifier;}
[0-9]+                 {printf("\t\t step = %d : i/p scan: %d\n\n",++step,atoi(yytext));yylval.num = atoi(yytext); return number;}
[ \t\n]                ;
[*+;]           	   {printf("\t\t step = %d : i/p scan: %c\n\n",++step,yytext[0]);return yytext[0];}
[=]					   {return yytext[0];}
.                      {ECHO; yyerror ("unexpected character");}

%%
int yywrap (void) {return 1;}
